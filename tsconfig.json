{
    "compilerOptions": {
      "target": "ES5",
      "module": "ESNext",
      "jsx": "react",
      "strict": true,
      "moduleResolution": "node",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "declaration": true,
      "outDir": "./build"
    },
    "include": ["src/**/*"]
  }
  